        },
        {
          "background": "0",
          "foreground": "15",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "precision": 2
          },
          "style": "diamond",
          "template": "Ô°ö {{ round .PhysicalPercentUsed .Precision }}% ",
          "trailing_diamond": "ÓÇº",
          "type": "sysinfo"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "0",
          "template": "‚îÄ‚ïØ"
        }
      ],
      "type": "rprompt"
    }
  ],
  "valid_line": {
    "background": "transparent",
    "foreground": "4"
  },
  "error_line": {
    "background": "transparent",
    "foreground": "1"
  },
  "osc99": true,
  "palette": {
    "nord0": "#2E3440",
    "nord1": "#3B4252",
    "nord10": "#5E81AC",
    "nord11": "#BF616A",
    "nord12": "#D08770",
    "nord13": "#EBCB8B",
    "nord14": "#A3BE8C",
    "nord15": "#B48EAD",
    "nord2": "#434C5E",
    "nord3": "#4C566A",
    "nord4": "#D8DEE9",
    "nord5": "#E5E9F0",
    "nord6": "#ECEFF4",
    "nord7": "#8FBCBB",
    "nord8": "#88C0D0",
    "nord9": "#81A1C1"
  },
  "version": 2
}
