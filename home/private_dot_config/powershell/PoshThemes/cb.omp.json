{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "0",
          "template": "‚ï≠‚îÄ"
        },
        {
          "background": "7",
          "foreground": "8",
          "leading_diamond": "ÓÇ∫",
          "powerline_symbol": "ÓÇº",
          "style": "diamond",
          "template": " Óúè {{if .Root}}‚ö° {{end}}",
          "type": "session"
        },
        {
          "background": "4",
          "foreground": "0",
          "powerline_symbol": "ÓÇº",
          "properties": {
            "folder_icon": "üìÇ",
            "folder_separator_icon": "/",
            "home_icon": "üè† ~",
            "mapped_locations": {
              "C:/Users/leoca/Development": "ü§ñ ~/dev",
              "C:/Users/leoca/Work": "üíº ~/work",
              "D:/leoca/Development": "ü§ñ ~/dev",
              "D:/leoca/Work": "üíº ~/work"
            },
            "mapped_locations_enabled": true,
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "0",
          "foreground": "5",
          "powerline_symbol": "ÓÇº",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "powerline",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} Ôöí {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} ÔÜª {{ .WorktreeCount }}{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "0",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": "{{ if eq .Code 0 }}<2>‚úî</>{{else}}<1>{{.Code}} ‚úò</>{{ end }}",
          "type": "exit"
        },
        {
          "background": "3",
          "foreground": "0",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "always_enabled": false,
            "style": "austin",
            "threshold": 3000
          },
          "style": "powerline",
          "template": "{{ .FormattedMs }} Ôâí",
          "type": "executiontime"
        },
        {
          "background": "2",
          "foreground": "0",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "display_mode": "context"
          },
          "style": "powerline",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} Ô†ü",
          "type": "python"
        },
        {
          "background": "15",
          "foreground": "0",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }}üïû",
          "trailing_diamond": "ÓÇº",
          "type": "time"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "0",
          "template": "‚îÄ‚ïÆ"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "0",
          "template": "‚ï∞‚îÄ"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "2",
          "foreground": "0",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∫",
          "properties": {
            "precision": 2
